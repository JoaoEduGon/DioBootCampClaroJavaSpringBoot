package SmartPhone;

import java.util.Scanner;

public class Iphone {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
	
		
		Iphone iphone = new Iphone();
		
		int ligado = sc.nextInt();
		
		
		if (ligado == 1) {
			iphone.Ligar();
			int Desbloquear = sc.nextInt();
			int Desligar = sc.nextInt();
			
			if (Desbloquear == 2) {
				iphone.Desbloquar();
			}else if(Desligar == 3){
				iphone.Desligar();
			}
			
		}
			
		
		
	}   

	public void Ligar() {
		System.out.println("SmartPhone Ligado");
	}
	
	public void Desligar() {
		System.out.println("SmartPhone desligado");
	}
	
	public void Desbloquar() {
		System.out.println("SmartPhone desbloueado");
	}
}
