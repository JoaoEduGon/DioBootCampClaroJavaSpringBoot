package SmartPhone;

public class Iphone {
	
	public static void main(String[] args) {
	
		
		Iphone iphone = new Iphone();
		
		int ligado = 1;
		int Desbloquear = 2;
		int Desligar = 3;
		
		
		if (ligado == 1) {
			iphone.Ligar();
			if (Desbloquear == 2) {
				iphone.Desbloquar();
			}else if(Desligar == 3){
				iphone.Desligar();
			}
			
		}
			
		
		
	}   

	public void Ligar() {
		System.out.println("SmartPhone Ligado");
	}
	
	public void Desligar() {
		System.out.println("SmartPhone desligado");
	}
	
	public void Desbloquar() {
		System.out.println("SmartPhone desbloueado");
	}
}
